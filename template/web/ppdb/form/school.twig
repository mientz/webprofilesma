<fieldset>
    <legend>Asal Sekolah</legend>
    <div class="form-group has-feedback">
        <label class="control-label">Nama Sekolah</label>
        <input class="form-control form-upcase typeahead-smp" type="text" name="school[name]" data-required-error="Wajib di-isi" required>
        <div class="help-block with-errors"></div>
        <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
    </div>
    <div class="form-group has-feedback">
        <label class="control-label">Kabupaten / Kota Madya Sekolah</label>
        <input class="form-control form-upcase typeahead-city" type="text" name="school[city]" data-required-error="Wajib di-isi" required>
        <div class="help-block with-errors"></div>
        <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
    </div>
    <div class="form-group has-feedback">
        <label class="control-label">Alamat Sekolah</label>
        <textarea class="form-control form-upcase" rows="3"  name="school[address]"></textarea>
    </div>
    <div class="form-group pull-right ">
        <button type="button" class="btn btn-default wiz-prev"><i class="fa fa-chevron-left fa-fw"></i>Data Keluarga</button>
        <button type="button" class="btn btn-primary wiz-next">Nilai Ujian Nasional<i class="fa fa-chevron-right fa-fw"></i></button>
    </div>
</fieldset>